{
  "name": "unibeautify-beautifier",
  "version": "0.1.3",
  "description": "Wrapper for a Unibeautify Beautifier ",
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "scripts": {
    "build": "npm run compile",
    "compile": "tsc",
    "test": "echo \"Error: no test specified\" && exit 0",
    "docs": "typedoc --out docs --includeDeclarations typings/*.ts --module commonjs src",
    "prepublish": "npm run compile",
    "lint:src": "tslint src/**/*.ts",
    "lint": "npm-run-all compile lint:*",
    "typings": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unibeautify/unibeautify-beautifier.git"
  },
  "keywords": [
    "beautify",
    "unibeautify",
    "wrapper"
  ],
  "author": "Glavin Wiechert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Unibeautify/unibeautify-beautifier/issues"
  },
  "homepage": "https://github.com/Unibeautify/unibeautify-beautifier#readme",
  "devDependencies": {
    "npm-run-all": "3.1.2",
    "tslint": "3.15.1",
    "typedoc": "0.4.5",
    "typescript": "2.9.2",
    "typings": "1.5.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.3.1"
  }
}
